Value LOCAL_INTERFACE (\w+\.\w+\.\w+)
Value CHASSIS_ID (\S+)
Value NEIGHBOR_PORT_ID (\S+)
Value NEIGHBOR_INTERFACE (\S+)
Value NEIGHBOR (\S+)
Value SYSTEM_DESCRIPTION (\S+)
Value CAPABILITIES (.*)
Value MANAGEMENT_IP (\S+)
#Value VLAN (\d+)
Value SERIAL (\S+)
Value MANUFACTURER (.*)
Value HW_REVISION (\S+)
Value FW_REVISION (\S+)
Value SW_REVISION (\S+)
Value MODEL (.*)
Value POWER_PAIR (\S+)
Value POWER_CLASS (\d+)
Value POWER_DEVICE_TYPE (\S+)
Value POWER_PRIORITY (\S+)
Value POWER_SOURCE (\S+)
Value POWER_REQUESTED (\d+)

Start
 ^Local\s${LOCAL_INTERFACE}
 ^\s\sChassis\sID\s\S+\s${CHASSIS_ID}
 ^\s\sPort\sID\s\.*\s${NEIGHBOR_PORT_ID}
 ^\s\sSystem\sName\s\S+\s${NEIGHBOR}
 ^\s\sManagement\sAddresses\s\S+\s${MANAGEMENT_IP}
 ^\s\sVLAN\sNames -> Record
 # Capture time-stamp if vty line has command time-stamping turned on
 ^Load\s+for\s+
 ^Time\s+source\s+is


