Value Required INTERFACE (\S+)
Value SWITCHPORT (.+?)
Value SWITCHPORT_MONITOR (.+?)
Value SWITCHPORT_NEGOTIATION (.+?)
Value MODE (\S+)
Value ADMIN_MODE (.+?)
Value ACCESS_VLAN (\d+|\d)
Value NATIVE_VLAN (\d+|\d)
Value VOICE_VLAN (\S+)
Value List TRUNKING_VLANS (\d+|\d)

Start
 ^\sInterface name\s+\:\s${INTERFACE}
 ^\sSwitchport mode\s+\:\s${MODE}
 ^\sIngress filter\s+\:\s\S+
 ^\sDefault Vlan\s+\:\s${NATIVE_VLAN}
 ^\sConfigured Vlans\s+\:\s+${TRUNKING_VLANS}$$ -> Record
 ^\sConfigured Vlans\s+\:\s+${TRUNKING_VLANS}\s+ -> Continue
 ^\sConfigured Vlans\s+\:\s+(?:\d+\s+){1}${TRUNKING_VLANS}\s+ -> Continue
 ^\sConfigured Vlans\s+\:\s+(?:\d+\s+){2}${TRUNKING_VLANS}\s+ -> Continue
 ^\sConfigured Vlans\s+\:\s+(?:\d+\s+){3}${TRUNKING_VLANS}\s+ -> Continue
 ^\sConfigured Vlans\s+\:\s+(?:\d+\s+){4}${TRUNKING_VLANS}\s+ -> Continue
 ^\sConfigured Vlans\s+\:\s+(?:\d+\s+){5}${TRUNKING_VLANS}\s+ -> Continue
 ^\sConfigured Vlans\s+\:\s+(?:\d+\s+){6}${TRUNKING_VLANS}\s+ -> Continue
 ^\sConfigured Vlans\s+\:\s+(?:\d+\s+){1}${TRUNKING_VLANS}$$ -> Continue
 ^\sConfigured Vlans\s+\:\s+(?:\d+\s+){2}${TRUNKING_VLANS}$$ -> Continue
 ^\sConfigured Vlans\s+\:\s+(?:\d+\s+){3}${TRUNKING_VLANS}$$ -> Continue
 ^\sConfigured Vlans\s+\:\s+(?:\d+\s+){4}${TRUNKING_VLANS}$$ -> Continue
 ^\sConfigured Vlans\s+\:\s+(?:\d+\s+){5}${TRUNKING_VLANS}$$ -> Continue
 ^\sConfigured Vlans\s+\:\s+(?:\d+\s+){6}${TRUNKING_VLANS}$$ -> Continue
 ^\s+(?:\d+\s+){0}${TRUNKING_VLANS}\s+ -> Continue
 ^\s+(?:\d+\s+){1}${TRUNKING_VLANS}\s+ -> Continue
 ^\s+(?:\d+\s+){2}${TRUNKING_VLANS}\s+ -> Continue
 ^\s+(?:\d+\s+){3}${TRUNKING_VLANS}\s+ -> Continue
 ^\s+(?:\d+\s+){4}${TRUNKING_VLANS}\s+ -> Continue
 ^\s+(?:\d+\s+){5}${TRUNKING_VLANS}\s+ -> Continue
 ^\s+(?:\d+\s+){0}${TRUNKING_VLANS}$$ -> Continue
 ^\s+(?:\d+\s+){1}${TRUNKING_VLANS}$$ -> Continue
 ^\s+(?:\d+\s+){2}${TRUNKING_VLANS}$$ -> Continue
 ^\s+(?:\d+\s+){3}${TRUNKING_VLANS}$$ -> Continue
 ^\s+(?:\d+\s+){4}${TRUNKING_VLANS}$$ -> Continue
 ^\s+(?:\d+\s+){5}${TRUNKING_VLANS}$$ -> Continue
 ^\sDynamic Vlans\s+ -> Record
# Capture time-stamp if vty line has command time-stamping turned on
# ^Load\s+for\s+
# ^Time\s+source\s+is

EOF

